{
  "name": "portfolio-server",
  "version": "0.0.1",
  "description": "Server for Przemyslaw Zydek portfolio page",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest -i",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "build": "tsc",
    "prod": "tsc && nodemon ./build/index.js"
  },
  "jest": {
    "rootDir": ".",
    "globals": {
      "__DEV__": true,
      "NODE_ENV": "development"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|styl|less|sass|scss)$": "identity-obj-proxy"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "author": {
    "name": "Przemysław Żydek",
    "email": "przemyslawzydek@gmail.com"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/express-fileupload": "^1.1.0",
    "@types/express-graphql": "^0.8.0",
    "@types/express-validator": "^3.0.0",
    "@types/faker": "^4.1.5",
    "@types/graphql": "^14.2.2",
    "@types/helmet": "0.0.43",
    "@types/http-status": "^0.2.30",
    "@types/jest": "^24.0.13",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.123",
    "@types/morgan": "^1.7.35",
    "@types/nodemailer": "^6.2.0",
    "@types/request-ip": "0.0.34",
    "@types/sanitize-html": "^1.20.0",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "^2.0.7",
    "@types/swagger-ui-express": "^3.0.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-jest": "^24.8.0",
    "eslint": "4.10.0",
    "jest": "^24.8.0",
    "node-mocks-http": "^1.7.5",
    "nodemon": "^1.17.5",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "socket.io-client": "^2.2.0",
    "supertest": "^4.0.2",
    "supports-color": "^6.1.0",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.5",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.5",
    "express-graphql": "^0.8.0",
    "express-validator": "^5.3.1",
    "faker": "^4.1.0",
    "graphql": "^14.4.2",
    "helmet": "^3.16.0",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongodb": "^3.2.7",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "nodemailer": "^6.2.1",
    "sanitize-html": "^1.20.1",
    "socket.io": "^2.2.0",
    "swagger-ui-express": "^4.0.2",
    "typeorm": "^0.2.18"
  },
  "license": "ISC"
}
